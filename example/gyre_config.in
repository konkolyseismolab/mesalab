! mesalab_config.in - Cleaned Version

&setup
  mesa_dir = '/home/tnehezd/workdir/mesa-r23.05.1/STRANGE/nad_convos_mid/'
  mesasdk_root = '/home/tnehezd/workdir/mesasdk'
  gyre_dir = '/home/tnehezd/workdir/mesa-r23.05.1/gyre/gyre'
  output_base_dir = './mesalab_results' ! Base directory for all outputs generated by mesalab
/

&run_control
  run_gyre = .TRUE.      ! Enable GYRE pulsation analysis.
/

&gyre_options
  enable_parallel = .TRUE.     ! Enable parallel execution of GYRE runs.
  num_gyre_threads = 2         ! Number of OpenMP threads (CPU cores) for each individual GYRE run.
  max_concurrent_gyre_runs = 3 ! Maximum number of GYRE runs to execute simultaneously (processes).
  gyre_inlist_template = 'gyre.in' ! Name of the base GYRE inlist template file.
  mesa_profile_pattern = 'LOGS/profile*.data.GYRE' ! Pattern to find MESA profile files within MESA model directories.
  mesa_profile_base_dir = '/home/tnehezd/workdir/mesa_blue_loop/mesalab/gyretools/mesalab_results' ! Absolute path to the directory containing MESA LOGS (model outputs).
  
  run_mode = 'FILTERED_PROFILES' ! 'FILTERED_PROFILES' to use models from CSV, 'ALL_PROFILES' for all found profiles.
  filtered_profiles_csv = '/home/tnehezd/workdir/mesa_blue_loop/output/analysis_results/sorted_mass_Z_min_max.csv' ! Path to CSV with model numbers for 'FILTERED_PROFILES' mode.
/

&analyze_options
  calc_stability = .TRUE.
  output_summary_csv = .TRUE.
/

&plot_options
  plot_hr = .TRUE.
  plot_k_diagram = .FALSE.
  plot_save_format = 'png'
  plot_dpi = 300
/